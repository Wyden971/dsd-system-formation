{
  "name": "dsd-system",
  "main": "src/index",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "updateSnapshots": "jest -u --coverage=false",
    "lint": "expo lint",
    "storybook-generate": "sb-rn-get-stories -c ./.ondevice",
    "storybook:web": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 7007",
    "storybook:ios": "cross-env STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "cross-env STORYBOOK_ENABLED='true' expo start --android"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/metro-config.js",
      "!**/tailwind.config.js"
    ]
  },
